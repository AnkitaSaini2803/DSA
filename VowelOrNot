class Solution{   
public:
    string isVowel(char c)
    {
         char a,e,i,o,u,A,E,I,O,U;
        if(c=='a'|| c=='e'|| c=='i'|| c=='o'|| c=='u'|| c=='A'|| c=='E'|| c=='I'|| c=='O'|| c=='U')
        {
            return "YES";
        }
        else
        {
            return "NO";
        }
        
      
        // code here 
    }
};
